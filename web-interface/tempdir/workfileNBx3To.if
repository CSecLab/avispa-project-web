section properties:

property secrecy_of_n0 (MGoal,ASGoal) :=
 [] ((secret(MGoal,n0,ASGoal) /\ iknows(MGoal))
     => contains(i,ASGoal))

property secrecy_of_n1 (MGoal,ASGoal) :=
