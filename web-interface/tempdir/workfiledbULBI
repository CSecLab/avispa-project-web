role router1 (R1, R2, RC :agent
             K1,K2,Kc   :public_key
             H:hash_func
SND, RCV                        : channel (dy)) 

played_by R1 def=


local
    Na  :text
    Tmp :nat

    
    sign         :hash(hash(text.text.text.text.text.text.text))

const
    request_id   : text,
    respond_id   : text,
    np, ns       : protocol_id

init State := 0

transition

 State   = 0
  SND(request_id )

 State' := 3

/ RCV(Sig.Sernb.Version.SeID.Sialg.VALp.Issn.Ext)

/ sign := H(Sernb'.Version'.SeID'.Sialg'.VALp'.Issn'.Ext')

/sig = sign


  end role




              