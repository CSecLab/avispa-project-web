role alice(A,B,S    : agent,
	   Kas      : symmetric_key,
	   SND, RCV : channel (dy))

played_by A def =
	local State: nat, Kab: symmetric_key
	init State := 0
transition

1. State = 0 /\ RCV({Kab'}_Kas) =|>
State':= 2 /\ SND({Kab'}_Kbs)

end role







goal

  %Alice authenticates Bob on na_nb1
  authentication_on Kab  % Addresses G1, G2, G3, G7, G10

end goal



environment()

