role alice(
A,B,S : agent,
PWA : symmetric_key,
Hash : hash_func,
SND,RCV : channel(dy))
played_by A def=
local
State : nat,
X,Z,P1 : text,
GY,Key : message
constsec_m_key: protocol_id
init
State := 0
transition
1. State = 0 /\ RCV(start) =|>
State':= 2 /\ Na' := new()
/\ SND({Na'}_K)
2. State = 2 /\ RCV({Nb'}_K) =|>
State':= 4 /\ K1' := Hash(Na.Nb')
/\ SND({Nb'}_K1')
/\ witness(A,B,bob_alice_nb,Nb')
end role
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

