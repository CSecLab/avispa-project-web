      => witness(A2Goal,A1Goal,cn_mn_n1,MGoal))
     /\ ((request(A1Goal,A2Goal,cn_mn_n1,MGoal,SID1)
           /\ request(A1Goal,A2Goal,cn_mn_n1,MGoal,SID2)
           /\ ~ equal(A2Goal,i))
