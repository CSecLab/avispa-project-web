property secrecy_of_sec_kcs1 (MGoal,ASGoal) :=
 [] ((secret(MGoal,sec_kcs1,ASGoal) /\ iknows(MGoal))
     => contains(i,ASGoal))

property secrecy_of_sec_kcs2 (MGoal,ASGoal) :=
