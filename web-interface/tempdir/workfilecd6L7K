role authenticator(A,S :agent,

Hash :hash_func,

Succ :hash_func,

SND,RCV :channel(dy)
)

played_by A def=

local

State :nat,

KEK :message,

Gtk,MIC,Pmk,Group,SN,KeyID ;message

init

State :=0

transition

1. State = 0 ? RCV(start) =|>

State':=2 ? Gtk' := Succ(Pmk) ? MIC' := Hash(Group.SN.KeyID.Gtk') ? SND({Group.SN.Gtk'.KeyID.MIC'}_KEK)

2. State = 2 ? RCV({Group.SN.MIC'}_KEK =|>

State' := 4

end role
