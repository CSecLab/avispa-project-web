section properties:

property secrecy_of_sna (MGoal,ASGoal) :=
 [] ((secret(MGoal,sna,ASGoal) /\ iknows(MGoal))
     => contains(i,ASGoal))

property secrecy_of_snb (MGoal,ASGoal) :=
